@model IEnumerable<FundacionDonandoParaAyudar.Web.Data.Entities.FoundationEntity>

    <div class="container">
        <div class="container">
            <div class="container">
                <hr />
                <h1 class="text-center mt-4" style="font-style:italic, oblique; font-family:Harabara;">
                    <b>Fundación Donando Para Ayudar</b>
                </h1>
                <hr />
                <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carouselExampleCaptions" data-slide-to="0" data-interval="4000" class="active"></li>
                        <li data-target="#carouselExampleCaptions" data-slide-to="1" data-interval="4000"></li>
                        <li data-target="#carouselExampleCaptions" data-slide-to="2" data-interval="4000"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="~/images/databanner1.png" class="img-responsive d-block w-100" alt="ASP NET"
                                 style="height:50%">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>First slide label</h5>
                                <p>Nulla vitae elit libero, a pharetra augue mollis interdum.</p>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <img src="~/images/databanner2.png" class="img-responsive d-block w-100" alt="ASP NET">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>Ayuda a los que mas lo necesitan!</h5>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <img src="~/images/databanner3.png" class="img-responsive d-block w-100" alt="ASP NET">
                            <div class="carousel-caption d-none d-md-block">
                                <h5>Third slide label</h5>
                                <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
                            </div>
                        </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
                <hr />
            </div>
        </div>
    </div>


    <div class="container text-center" style="font-style:italic, oblique; font-family:Harabara;">
        <img class="img-responsive" src="~/images/news.png" alt="Entertate" />
        <h1 class="text-center">Entérate    </h1>
    </div>
            <div class="container mt-4">
                <div class="container">
                    <div class="container">
                        <hr />
                        <div class="row">
                            <div class="col-md-4">
                                <div class="card mb-4 shadow-sm">
                                    <img class="card-img-top"
                                         src="~/images/fundacion.jpg"
                                         alt="About us" />
                                    <div class="card-body">
                                        <p class="card-text">
                                            <font style="vertical-align: inherit;">
                                                Somos una fundación sin animo de lucro, nos enfocamos en población en situación de vulnerabilidad, no sólo con ayudas materiales,sino integraciones sociales, interacciones ludico-recreativas.
                                            </font>
                                        </p>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="btn-group">
                                                <a asp-area="" asp-controller="Home" asp-action="About"
                                                   class="btn btn-sm btn-link">
                                                    <font style="vertical-align: inherit;">
                                                        <font style="vertical-align: inherit;">Conoce más aquí</font>
                                                    </font>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="card mb-4 shadow-sm">
                                    <img class="card-img-top"
                                         src="~/images/salidaFDPA.jpg"
                                         alt="Comentarios" />
                                    <div class="card-body">
                                        <p class="card-text">
                                            <font style="vertical-align: inherit;">
                                                Los comentarios que se han realizado a nuestra fundación
                                            </font>
                                        </p>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="btn-group">
                                                <a asp-area="" asp-controller="Foundation" asp-action="Index"
                                                   class="btn btn-sm btn-link">
                                                    <font style="vertical-align: inherit;">
                                                        <font style="vertical-align: inherit;">Conoce más aquí</font>
                                                    </font>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="card mb-4 shadow-sm">
                                    <img class="card-img-top"
                                         src="~/images/enterate.jpg"
                                         alt="Enterate" />
                                    <div class="card-body">
                                        <p class="card-text">
                                            <font style="vertical-align: inherit;">
                                                ¿Quieres conocer más de nosotros?.
                                            </font>
                                            <font style="vertical-align: inherit;">
                                                Accede a nuestra información de contacto, y nos puedes dejar un comentario
                                            </font>
                                        </p>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="btn-group">
                                                <a asp-area="" asp-controller="Home" asp-action="Contact"
                                                   class="btn btn-sm btn-link">
                                                    <font style="vertical-align: inherit;">
                                                        <font style="vertical-align: inherit;">Conoce más aquí</font>
                                                    </font>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr />
                </div>
             </div>

            <div class="container mt-2 text-center" style="font-style:italic, oblique; font-family:Harabara;">
                <button type="button" class="btn btn-link text-center"
                        data-container="body"
                        data-toggle="popover"
                        data-placement="top"
                        data-content="Podrás ver nuestros videos de la fundación">
                    <img class="img-responsive" alt="Youtube"
                         src="~/images/youtube.png" />
                </button>
                <h1 class="text-center">Videos</h1>
            </div>
            <div class="row">
                <div class="col-md-5 offset-md-1">
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item"
                                src="https://www.youtube.com/embed/akQoGtSVg10?autoplay=0&fs=0&iv_load_policy=3&showinfo=0&rel=0&cc_load_policy=0&start=0&end=0"
                                allowfullscreen></iframe>
                    </div>
                </div>
                <div class="col-md-5 offset-mr-auto">
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item"
                                src="https://www.youtube.com/embed/D4UBKu9ozgo?autoplay=0&fs=0&iv_load_policy=3&showinfo=0&rel=0&cc_load_policy=0&start=0&end=0"
                                allowfullscreen></iframe>
                    </div>
                </div>
            </div>
                    @section Scripts {
                        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
                    }
